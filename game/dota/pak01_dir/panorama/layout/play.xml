								<Label class="RankTierSoloCalibrating" text="#ranked_mmr_solo_calibration_games"/>
								<Label class="RankTierPartyCalibrating" text="#ranked_mmr_party_calibration_games"/>
							</Panel>
						</Panel>
						<Panel class="Divider" />						
						<Panel id="RankedPhoneStatusContainer">
